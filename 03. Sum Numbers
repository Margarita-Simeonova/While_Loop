def sum_numbers(cotrol_number, number):
    sum_of_numbers = 0
    
    while not sum_of_numbers >= cotrol_number:
        if sum_of_numbers > cotrol_number:
            break
        
        sum_of_numbers += number
        number = int(input())
    
    return sum_of_numbers
    
    
control_number_arg = int(input())
number_arg = int(input())

result = sum_numbers(control_number_arg, number_arg)
print(result)
